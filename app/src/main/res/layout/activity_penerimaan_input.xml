  <TextView
      android:id="@+id/textView6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="24dp"
      android:layout_marginTop="24dp"
      android:text="Yang dibayarkan anggota"
      android:textSize="15sp"
      android:textStyle="bold"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView3" />

  <TextView
      android:id="@+id/textView7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="24dp"
      android:layout_marginTop="10dp"
      android:text="Setoran Anggota"
      android:textSize="15sp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView6"
      tools:ignore="MissingConstraints" />

  <EditText
      android:id="@+id/setor_anggota"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:hint="Setoran Anggota"
      android:minHeight="48dp"
      android:singleLine="true"
      app:layout_constraintEnd_toEndOf="@+id/nama_anggota"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/nama_anggota"
      app:layout_constraintTop_toBottomOf="@+id/textView6"
      tools:ignore="MissingConstraints" />

  <TextView
      android:id="@+id/textView8"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="24dp"
      android:layout_marginTop="24dp"
      android:text="Jumlah Bayar"
      android:textSize="15sp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView7"
      tools:ignore="MissingConstraints" />

  <EditText
      android:id="@+id/jumlah_bayar_anggota"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:hint="Jumlah Bayar"
      android:minHeight="48dp"
      android:singleLine="true"
      app:layout_constraintEnd_toEndOf="@+id/nama_anggota"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/nama_anggota"
      app:layout_constraintTop_toBottomOf="@+id/setor_anggota"
      tools:ignore="MissingConstraints" />

  <TextView
      android:id="@+id/textView9"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="24dp"
      android:layout_marginTop="24dp"
      android:text="Bayar Minggon"
      android:textSize="15sp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView8"
      tools:ignore="MissingConstraints" />

  <EditText
      android:id="@+id/minggon_bayar_anggota"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:hint="Bayar Minggon"
      android:minHeight="48dp"
      android:singleLine="true"
      app:layout_constraintEnd_toEndOf="@+id/nama_anggota"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/nama_anggota"
      app:layout_constraintTop_toBottomOf="@+id/jumlah_bayar_anggota"
      tools:ignore="MissingConstraints" />

  <Button
      android:id="@+id/button3"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginEnd="8dp"
      android:text="Input Penerimaan"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/minggon_bayar_anggota" />
</androidx.constraintlayout.widget.ConstraintLayout>